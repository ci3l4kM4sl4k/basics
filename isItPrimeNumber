//Algorithm for checking if given number is even or odd for c++ language
#include <iostream>
using namespace std;

////////////////////
void isItPrime(int n)
{
    if(n == 0)
        cout << "This number is 0";
    else if(n == 1)
        cout << "This number is 1";
    else
    {
        while(n >= 2)
    }
}
////////////////////

int main()
{
    int a = 26211;

    isItPrime(a);

    return 0;
}
